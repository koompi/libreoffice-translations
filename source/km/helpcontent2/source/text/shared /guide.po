msgctxt ""
"language_select.xhp\n"
"par_id3156283\n"
"help.text"
msgid "Select the <emph>Language</emph> and click <emph>OK</emph>."
msgstr "Select the <emph>Language</emph> and click <emph>OK</emph>."

