msgctxt ""
"04060183.xhp\n"
"par_id2909778\n"
"help.text"
msgid "<item type=\"input\">=LOGNORM.DIST(0.1;0;1;1)</item> returns 0.0106510993."
msgstr "<item type=\"input\">=LOGNORMDIST(0.1;0;1)</item> ត្រឡប់ 0.01 ។"

