#. 4cCHs
#: replace.xhp
msgctxt ""
"replace.xhp\n"
"par_id931552552227310\n"
"help.text"
msgid "Replace (Text As String, SearchStr As String, ReplStr As String [, Start As Long [, Count as Long [, Compare As Boolean]]]"
msgstr "ជំនួស (អត្ថបទជាខ្សែអក្សរ, SearchStr ក្នុងនាមជាខ្សែអក្សរ, ReplStr ជាខ្សែអក្សរ [, ចាប់ផ្តើមឱ្យវែង [, រាប់ជាយូរមកហើយ [, ប្រៀបធៀបជាប៊ូលីន]]]"

#. iMDGH
#: replace.xhp
msgctxt ""
"replace.xhp\n"
"par_id911552552252024\n"
"help.text"
msgid "String"
msgstr "ខ្សែអក្សរ"

#. gZGmH
#: replace.xhp
msgctxt ""
"replace.xhp\n"
"par_id721552552263062\n"
"help.text"
msgid "<emph>Text:</emph> Any string expression that you want to modify."
msgstr "<emph> អត្ថបទ៖ </emph> កន្សោមខ្សែអក្សរណាមួយដែលអ្នកចង់កែប្រែ។"

#. JdTUJ
#: replace.xhp
msgctxt ""
"replace.xhp\n"
"par_id901552552269836\n"
"help.text"
msgid "<emph>SearchStr:</emph> Any string expression that shall be searched for."
msgstr "<emph> SearchStr: </emph> កន្សោមខ្សែអក្សរណាមួយដែលត្រូវស្វែងរក។"

#. yK8dz
#: replace.xhp
msgctxt ""
"replace.xhp\n"
"par_id791552552275383\n"
"help.text"
msgid "<emph>ReplStr:</emph> Any string expression that shall replace the found search string."
msgstr "<emph> ReplStr: </emph> កន្សោមខ្សែអក្សរណាមួយដែលនឹងជំនួសខ្សែអក្សរស្វែងរកដែលបានរកឃើញ"

#. y6ZZR
#: replace.xhp
msgctxt ""
"replace.xhp\n"
"par_id111552552283060\n"
"help.text"
msgid "<emph>Start:</emph> Numeric expression that indicates the character position where the search starts and also the start of the substring to be returned."
msgstr "<emph> ចាប់ផ្តើម៖ </emph> កន្សោមលេខដែលបង្ហាញពីទីតាំងតួអក្សរដែលការស្វែងរកចាប់ផ្តើមហើយក៏ជាការចាប់ផ្តើមនៃខ្សែរងដែលត្រូវបញ្ជូនត្រឡប់មកវិញ។"

#. bLh8G
#: replace.xhp
msgctxt ""
"replace.xhp\n"
"par_id921552552289833\n"
"help.text"
msgid "<emph>Count:</emph> The maximal number of times the replace shall be performed."
msgstr "<emph> រាប់៖ </emph> ចំនួនដងអតិបរមាដែលត្រូវជំនួស។"

#. Pwrb8
#: replace.xhp
msgctxt ""
"replace.xhp\n"
"par_id891552552302894\n"
"help.text"
msgid "<emph>Compare:</emph> Optional boolean expression that defines the type of comparison. The value of this parameter can be TRUE or FALSE. The default value of TRUE specifies a text comparison that is not case-sensitive. The value of FALSE specifies a binary comparison that is case-sensitive. You can as well use 0 instead of FALSE or 1 instead of TRUE."
msgstr "<emph> ប្រៀបធៀប៖ </emph> កន្សោមប៊ូលីនស្រេចចិត្តដែលកំណត់ប្រភេទប្រៀបធៀប។ តម្លៃនៃប៉ារ៉ាម៉ែត្រនេះអាចជាពិតឬមិនពិត។ តម្លៃលំនាំដើមរបស់ TRUE បញ្ជាក់ការប្រៀបធៀបអត្ថបទដែលមិនប្រកាន់អក្សរតូចធំ។ តម្លៃនៃ FALSE បញ្ជាក់ការប្រៀបធៀបគោលពីរដែលប្រកាន់អក្សរតូចធំ។ អ្នកក៏អាចប្រើ ០ ជំនួស FALSE ឬ ១ ជំនួស TRUE ដែរ។"

#. sFyGa
#: replace.xhp
msgctxt ""
"replace.xhp\n"
"par_id991552552420717\n"
"help.text"
msgid "msgbox replace (\"aBbcnnbnn\", \"b\", \"$\", 1, 1, FALSE)'returns \"aB$cnnbnn\""
msgstr "msgbox ជំនួស (\"aBbcnnbnn \", \"b \", \"$\", ១, ១, FALSE) &#39;ត្រឡប់ \"aB $cnnbnn \""

#. ZHjzn
#: replace.xhp
msgctxt ""
"replace.xhp\n"
"par_id321552552440672\n"
"help.text"
msgid "REM meaning: \"b\" should be replaced, but"
msgstr "អត្ថន័យ REM៖ \"b \" គួរតែត្រូវបានជំនួសប៉ុន្តែ"

#. BZgJy
#: replace.xhp
msgctxt ""
"replace.xhp\n"
"par_id571552552467647\n"
"help.text"
msgid "REM * only when lowercase (parameter 6), hence second occurrence of \"b\""
msgstr "REM * តែនៅពេលដែលអក្សរតូច (ប៉ារ៉ាម៉ែត្រ ៦) ដូច្នេះការកើតឡើងជាលើកទីពីរនៃ \"b \""

#. HubEe
#: replace.xhp
msgctxt ""
"replace.xhp\n"
"par_id71552552474769\n"
"help.text"
msgid "REM * only first (respecting case) occurrence (parameter 5)"
msgstr "REM * មានតែករណីទីមួយប៉ុណ្ណោះ (ករណីគោរព) កើតឡើង (ប៉ារ៉ាម៉ែត្រ ៥)"

#. QEPtG
#: replace.xhp
msgctxt ""
"replace.xhp\n"
"par_id861587778446685\n"
"help.text"
msgid "REM returns D*FGHI because the search starts at position 4, which is also the start of the returned string."
msgstr "REM ត្រឡប់ D * FGHI ពីព្រោះការស្វែងរកចាប់ផ្តើមនៅទីតាំងទី 4 ដែលជាការចាប់ផ្តើមនៃខ្សែត្រឡប់មកវិញ។"

