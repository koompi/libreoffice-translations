msgstr "MyNumber = ៥ 'ត្រឹមត្រូវ'"

#. BQYCs
#: 01020100.xhp
msgctxt ""
"01020100.xhp\n"
"par_idm1341267456\n"
"help.text"
msgid "MyNumber5=15    'Correct'"
msgstr "MyNumber5=15    'Correct'"

#. Pvdzr
#: 01020100.xhp
msgctxt ""
"01020100.xhp\n"
"par_idm1341262016\n"
"help.text"
msgid "MyNumber_5=20   'Correct'"
msgstr "MyNumber_5 = ២០ 'ត្រឹមត្រូវ'"

#. JfvAk
#: 01020100.xhp
msgctxt ""
"01020100.xhp\n"
"par_idm1341256576\n"
"help.text"
msgid "My Number=20    'Not valid, variable with space must be enclosed in square brackets'"
msgstr "លេខរបស់ខ្ញុំ = ២០ 'មិនមានសុពលភាពអថេរដែលមានចន្លោះត្រូវដាក់ជាដង្កៀបជ្រុង'"

